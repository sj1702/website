<script type="text/javascript">
document.addEventListener('DOMContentLoaded', function() {
    var acceptanceText = document.getElementById('span.wpcf7-form-control.wpcf7-acceptance span.wpcf7-list-item-label');
    var acceptanceCheckbox = document.getElementById('acceptance');

    if (acceptanceText && acceptanceCheckbox) {
        acceptanceText.addEventListener('click', function(event) {
            // Prevent the event from reaching the checkbox
            event.stopPropagation();

            // Manually toggle the checkbox state
            acceptanceCheckbox.checked = !acceptanceCheckbox.checked;
        });

        // Prevent the default click action on the checkbox itself
        acceptanceCheckbox.addEventListener('click', function(event) {
            event.preventDefault();
        });
    }
});
	
	</script>


